# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-project-url.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Analytics (Choisir une ou plusieurs options)

# Google Analytics 4
NEXT_PUBLIC_GA_ID=G-XXXXXXXXXX

# Vercel Analytics (Activé automatiquement si déployé sur Vercel)
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# Plausible Analytics
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=holding.phistudios.com

# Error Tracking

# Sentry
NEXT_PUBLIC_SENTRY_DSN=https://xxxxx@oyyy.ingest.sentry.io/zzzzz
SENTRY_AUTH_TOKEN=your-sentry-auth-token
SENTRY_ORG=phi-studios
SENTRY_PROJECT=holding-manager

# Logging

# Custom logs endpoint (optionnel)
NEXT_PUBLIC_LOGS_ENDPOINT=/api/logs

# Log level (info, warn, error, debug)
LOG_LEVEL=info

# Application

# Environment
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Features flags (optionnel)
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_TRACKING=true
